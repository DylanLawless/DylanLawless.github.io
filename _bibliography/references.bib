	Journal = {The American Journal of Human Genetics},
	Keywords = {network analysis},
	Number = {2},
	Pages = {224 - 237},
	Title = {Optimal Unified Approach for Rare-Variant Association Testing with Application to Small-Sample Case-Control Whole-Exome Sequencing Studies},
	Url = {http://www.sciencedirect.com/science/article/pii/S0002929712003163},
	Volume = {91},
